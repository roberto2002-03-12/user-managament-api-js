{
  "name": "user-managament-api",
  "version": "1.0.0",
  "description": "this api can be used for any project, but i recommend you to modify this code for your purpose.",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "lint": "eslint",
    "test": "jest",
    "migration:generate": "sequelize-cli migration:generate --name",
    "migration:run": "sequelize-cli db:migrate",
    "migration:revert": "sequelize-cli db:migrate:undo",
    "migration:delete": "sequelize-cli db:migrate:undo:all",
    "seed:all": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo:all",
    "e2e": "cross-env NODE_ENV=e2e jest --config ./jest-e2e.json --verbose --detectOpenHandles --forceExit",
    "e2e:coverage": "jest --config ./jest-e2e.json --verbose --detectOpenHandles --forceExit --coverage"
  },
  "keywords": [
    "user",
    "managament",
    "javascript",
    "node",
    "express"
  ],
  "author": "Roberto Contreras Gonzales",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.347.0",
    "@hapi/boom": "^10.0.1",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "mysql2": "^3.3.3",
    "nodemailer": "^6.9.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.32.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "eslint": "^8.42.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "sequelize-cli": "^6.6.0",
    "supertest": "^6.3.3",
    "umzug": "^3.2.1"
  }
}
